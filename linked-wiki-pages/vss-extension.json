{
  "manifestVersion": 1.0,
  "id": "linked-wiki-pages",
  "version": "0.0.24",
  "name": "Linked Wiki pages",
  "description": "Displays the list of all Wiki pages linked to current Work Item.",
  "publisher": "boykoant",
  "galleryFlags": [
    "Public",
    "Preview"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "src/img/icon.png"
  },
  "contributions": [
    {
      "id": "linked-wiki-pages",
      "type": "ms.vss-work-web.work-item-form-group",
      "description": "Displays the list of all Wiki pages linked to current Work Item.",
      "targets": [
        "ms.vss-work-web.work-item-form"
      ],
      "properties": {
        "name": "Linked wiki pages",
        "uri": "linked-wiki-pages.html",
        "height": 200
      }
    }
  ],
  "categories": [
    "Plan and track"
  ],
  "tags": [
    "wiki"
  ],
  "content": {
    "details": {
      "path": "docs/overview.md"
    }
  },
  "links": {
    "repository": {
      "uri": "https://github.com/TFSExt/Forms"
    },
    "issues": {
      "uri": "https://github.com/TFSExt/Forms/issues"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/TFSExt/Forms"
  },
  "files": [
    {
      "path": "src/linked-wiki-pages.html",
      "addressable": true
    },
    {
      "path": "src/node_modules/vss-web-extension-sdk/lib",
      "addressable": true,
      "packagePath": "vsts"
    },
    {
      "path": "src/node_modules/knockout/build/output",
      "addressable": true,
      "packagePath": "knockout"
    },
    {
      "path": "src/js",
      "addressable": true,
      "packagePath": "js"
    },
    {
      "path": "docs/screenshots",
      "addressable": true,
      "packagePath": "screenshots"
    }
  ]
}

